{{>headerDark}}
<main class="main">
	<section class="page_block ">
		<div class="hero">
			{{#with hero}}
			{{>hero title=title text=description}}
			<a class="button" href="{{button.url}}">{{button.text}}</a>
			{{/with}}
		</div>
	</section>
</main>
<div class="card">
	{{#with about }}
	<div class="card_title">
		<h1>{{heading}}</h1>
	</div>
	<div class="card_text">
		{{info}}
	</div>
	{{/with}}
</div>
<div class="info">
	<div class="info_title">
		<h1>Our Articles</h1>
	</div>
	<div class="info_logo ">
		<span class="info_badge ">View All</span>
	</div>
</div>
{{>articleCardDark}}
{{>footerDark}}